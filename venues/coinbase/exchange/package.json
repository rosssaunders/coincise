{
  "name": "@coincise/coinbase",
  "version": "2.0.0",
  "description": "Documentation extraction for Coinbase Exchange API",
  "type": "module",
  "scripts": {
    "extract:general": "node src/extractGeneral.js",
    "extract:endpoints": "node src/extractEndpoints.js",
    "extract:all": "npm run extract:general && npm run extract:endpoints",
    "legacy:privaterestapi": "node src/index.js batch -c config/private_rest_api.json",
    "legacy:publicrestapi": "node src/index.js batch -c config/public_rest_api.json",
    "legacy:websocketapi": "node src/index.js batch -c config/websocket_api.json",
    "legacy:all": "npm run legacy:privaterestapi && npm run legacy:publicrestapi && npm run legacy:websocketapi"
  },
  "keywords": [
    "coinbase",
    "cryptocurrency",
    "exchange",
    "api",
    "documentation"
  ],
  "author": "Coincise",
  "license": "MIT",
  "dependencies": {
    "jsdom": "27.0.0",
    "puppeteer": "24.22.0",
    "turndown": "7.2.1",
    "turndown-plugin-gfm": "1.0.2"
  }
}
