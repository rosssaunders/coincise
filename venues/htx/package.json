{
  "name": "htx-api-docs-extractor",
  "version": "1.0.0",
  "description": "HTX API Documentation Extractor",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "extract:general": "node src/extractGeneral.js",
    "extract:endpoints": "node src/extractEndpoints.js",
    "extract:all": "npm run extract:general && npm run extract:endpoints",
    "start": "node src/index.js",
    "privaterestspot": "node src/index.js config/spot/private_rest_api.json",
    "publicrestspot": "node src/index.js config/spot/public_rest_api.json",
    "privatewebsocketspot": "node src/index.js config/spot/private_websocket_api.json",
    "publicwebsocketspot": "node src/index.js config/spot/public_websocket_api.json",
    "fixspot": "node src/index.js config/spot/fix_api.json",
    "privaterestusdtm": "node src/index.js config/usdtm/private_rest_api.json",
    "publicrestusdtm": "node src/index.js config/usdtm/public_rest_api.json",
    "privatewebsocketusdtm": "node src/index.js config/usdtm/private_websocket_api.json",
    "publicwebsocketusdtm": "node src/index.js config/usdtm/public_websocket_api.json",
    "privaterestcoinm": "node src/index.js config/coinm/private_rest_api.json",
    "publicrestcoinm": "node src/index.js config/coinm/public_rest_api.json",
    "runall": "npm run privaterestspot && npm run publicrestspot && npm run privatewebsocketspot && npm run publicwebsocketspot && npm run fixspot && npm run privaterestusdtm && npm run publicrestusdtm && npm run privatewebsocketusdtm && npm run publicwebsocketusdtm && npm run privaterestcoinm && npm run publicrestcoinm"
  },
  "dependencies": {
    "jsdom": "27.0.0",
    "puppeteer": "^24.22.0",
    "turndown": "^7.2.1",
    "turndown-plugin-gfm": "^1.0.2",
    "prettier": "3.6.2"
  },
  "devDependencies": {
    "nodemon": "3.1.10",
    "prettier": "3.6.2",
    "prettier-plugin-java": "2.7.5"
  }
}
