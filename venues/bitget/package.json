{
  "name": "bitget-api-converter",
  "version": "1.0.0",
  "description": "Convert Bitget API documentation to markdown",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "process:common": "node src/index.js config/common.json",
    "process:spot": "node src/index.js config/spot.json",
    "process:future": "node src/index.js config/future.json",
    "start": "npm run process:common && npm run process:spot && npm run process:future",
    "start:sequential": "node src/utils/run-sequential.js"
  },
  "dependencies": {
    "axios": "1.9.0",
    "cheerio": "1.0.0",
    "puppeteer": "^24.8.0",
    "turndown": "7.2.0",
    "turndown-plugin-gfm": "1.0.2"
  }
}
