{
  "name": "@coincise/binancespot",
  "version": "1.0.0",
  "description": "Documentation extraction for Binance Spot API",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "extract:general": "node src/extractGeneral.js",
    "extract:endpoints:public_rest": "node src/index.js ./config/public_rest.json",
    "extract:endpoints:private_rest": "node src/index.js ./config/private_rest.json",
    "extract:endpoints:public_websocket": "node src/index.js ./config/public_websocket.json",
    "extract:endpoints:private_websocket": "node src/index.js ./config/private_websocket.json",
    "extract:endpoints:fix": "node src/index.js ./config/fix.json",
    "extract:endpoints:sbe": "node src/index.js ./config/sbe.json",
    "extract:all": "npm run extract:general && npm run extract:endpoints:public_rest && npm run extract:endpoints:private_rest && npm run extract:endpoints:public_websocket && npm run extract:endpoints:private_websocket && npm run extract:endpoints:fix && npm run extract:endpoints:sbe"
  },
  "keywords": [
    "binance",
    "cryptocurrency",
    "exchange",
    "api",
    "documentation"
  ],
  "author": "Coincise",
  "license": "MIT",
  "dependencies": {
    "puppeteer": "24.22.0",
    "turndown": "7.2.1",
    "turndown-plugin-gfm": "^1.0.2",
    "jsdom": "27.0.0"
  }
}
