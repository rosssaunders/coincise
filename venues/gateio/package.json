{
  "name": "gateio-scraper",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
  "start": "npm-run-all --sequential extract:*",
  "startWs": "npm-run-all --sequential extract:websocket-*",
  "extract:change-log": "node src/rest_api.js config/change_log.json",
  "extract:general": "node src/rest_api.js config/general.json",
  "extract:account": "node src/rest_api.js config/account_private_rest_api.json",
  "extract:subaccount": "node src/rest_api.js config/subaccount_private_rest_api.json",
  "extract:wallet": "node src/rest_api.js config/wallet_private_rest_api.json",
  "extract:withdrawal": "node src/rest_api.js config/withdrawal_private_rest.json",
  "extract:unified": "node src/rest_api.js config/unified_private_rest_api.json",
  "extract:spot": "node src/rest_api.js config/spot_private_rest_api.json",
  "extract:options": "node src/rest_api.js config/options_private_rest_api.json",
  "extract:futures": "node src/rest_api.js config/futures_private_rest_api.json",
  "extract:delivery": "node src/rest_api.js config/delivery_private_rest_api.json",
  "extract:websocket-spot": "node src/websocket_spot_api.js",
  "extract:websocket-delivery": "node src/websocket_delivery_api.js",
  "extract:websocket-futures": "node src/websocket_futures_api.js",
  "extract:websocket-options": "node src/websocket_options_api.js",
  "extract:websocket-unified": "node src/websocket_unified_api.js"
  },
  "dependencies": {
    "axios": "1.9.0",
    "cheerio": "^1.0.0",
    "dotenv": "16.5.0",
    "jsdom": "26.1.0",
    "turndown": "^7.1.2",
    "turndown-plugin-gfm": "^1.0.2"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
