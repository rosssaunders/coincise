{
  "name": "gateio-scraper",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
  "start": "npm-run-all --sequential extract:*",
  "startWs": "npm-run-all --sequential extract:websocket-*",
  "extract:change-log": "node src/rest_api.js config/change_log.json",
  "extract:general": "node src/rest_api.js config/general.json",
  "extract:account": "node src/rest_api.js config/private_rest_api_account.json",
  "extract:subaccount": "node src/rest_api.js config/private_rest_api_subaccount.json",
  "extract:collateral-loan": "node src/rest_api.js config/private_rest_api_collateral_loan.json",
  "extract:multi-collateral-loan": "node src/rest_api.js config/private_rest_api_multi_collateral_loan.json",
  "extract:isolated-margin": "node src/rest_api.js config/private_rest_api_isolated_margin.json",
  "extract:earn": "node src/rest_api.js config/private_rest_api_earn.json",
  "extract:earnuni": "node src/rest_api.js config/private_rest_api_earnuni.json",
  "extract:flash-swap": "node src/rest_api.js config/private_rest_api_flash_swap.json",
  "extract:rebate": "node src/rest_api.js config/private_rest_api_rebate.json",
  "extract:wallet": "node src/rest_api.js config/private_rest_api_wallet.json",
  "extract:withdrawal": "node src/rest_api.js config/private_rest_api_withdrawal.json",
  "extract:unified": "node src/rest_api.js config/private_rest_api_unified.json",
  "extract:spot": "node src/rest_api.js config/private_rest_api_spot.json",
  "extract:options": "node src/rest_api.js config/private_rest_api_options.json",
  "extract:futures": "node src/rest_api.js config/private_rest_api_futures.json",
  "extract:delivery": "node src/rest_api.js config/private_rest_api_delivery.json",
  "extract:websocket-spot": "node src/websocket_spot_api.js",
  "extract:websocket-delivery": "node src/websocket_delivery_api.js",
  "extract:websocket-futures": "node src/websocket_futures_api.js",
  "extract:websocket-options": "node src/websocket_options_api.js",
  "extract:websocket-unified": "node src/websocket_unified_api.js"
  },
  "dependencies": {
    "axios": "1.12.0",
    "cheerio": "^1.0.0",
    "dotenv": "16.5.0",
    "jsdom": "26.1.0",
    "turndown": "^7.1.2",
    "turndown-plugin-gfm": "^1.0.2"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
