{
  "name": "upbit-monitor",
  "version": "1.0.0",
  "description": "Upbit API documentation monitor",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "extract:assets": "node src/index.js config/assets.json",
    "extract:order": "node src/index.js config/order.json",
    "extract:withdrawal": "node src/index.js config/withdrawal.json",
    "extract:deposit": "node src/index.js config/deposit.json",
    "extract:service": "node src/index.js config/service.json",
    "extract:quotation": "node src/index.js config/quotation.json",
    "extract:websocket": "node src/index.js config/websocket.json",
    "extract:all": "pnpm run extract:assets && pnpm run extract:order && pnpm run extract:withdrawal && pnpm run extract:deposit && pnpm run extract:service && pnpm run extract:quotation && pnpm run extract:websocket",
    "extract:changelog": "node src/index.js config/private.json",
    "format": "node -e \"import('../../shared/format-markdown.js').then(({formatMarkdown}) => formatMarkdown('../../docs/upbit/change_log.md'))\""
  },
  "dependencies": {
    "jsdom": "27.0.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.6.2",
    "puppeteer": "24.22.0",
    "turndown": "^7.2.1",
    "turndown-plugin-gfm": "^1.0.2"
  }
}