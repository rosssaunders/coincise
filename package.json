{
  "name": "coincise",
  "version": "1.0.0",
  "description": "Concise Crypto Currency Exchange APIs",
  "scripts": {
    "format": "prettier --write \"**/*.{js,md}\"",
    "site:generate-llms": "node tools/llms/generate-llms-enhanced.js",
    "site:generate-json": "node tools/llms/generate-json-index.js",
    "site:generate": "npm run site:generate-llms && npm run site:generate-json",
    "site:build": "./tools/website/build.sh",
    "site:dev": "cd site && mkdocs serve --dev-addr localhost:8000"
  },
  "dependencies": {
    "jsdom": "^27.2.0",
    "json5": "^2.2.3",
    "node-fetch": "^3.3.2",
    "puppeteer": "^24.31.0",
    "turndown": "^7.2.2",
    "turndown-plugin-gfm": "^1.0.2"
  },
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2"
  },
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd",
