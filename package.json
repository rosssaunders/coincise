      "form-data@>=3.0.0 <3.0.4": ">=3.0.4",
      "ws@>=8.0.0 <8.17.1": ">=8.17.1",
      "tar-fs@>=3.0.0 <3.1.1": ">=3.1.1",
      "yargs-parser@>=6.0.0 <13.1.2": ">=13.1.2",
      "markdown-it@<12.3.2": ">=12.3.2",
      "ajv@<6.12.3": ">=6.12.3",
      "jsonpointer@<5.0.0": ">=5.0.0"
    }
